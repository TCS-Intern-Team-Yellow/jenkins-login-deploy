pipeline {
	agent any
	stages {
		stage('Sample') {
			steps {
				sh 'whoami'
				sh 'sudo -S usermod -a -G docker jenkins'
				git branch: '20220503', url: 'https://github.com/TCS-Intern-Team-Yellow/login.git'
				sh 'ls'
				sh 'docker build -t loginservice .'
				sh 'docker images ls'
			}
		}
	}
}
